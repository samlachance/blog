<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'application', 'https://fonts.googleapis.com/css?family=Playfair+Display|Scope+One' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
</head>
<body>
<div class="wrapper">
  <%= render partial: "shared/header" %>
  <%= render partial: "shared/sidebar" %>
  <div class="content">
    <%= yield %>
  </div>
</div>

</body>
</html>
